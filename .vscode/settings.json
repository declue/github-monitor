{
    "files.associations": {
        "*.py": "python"
    },
    "python.defaultInterpreterPath": ".venv/Scripts/python.exe",
    "files.exclude": {
        "**/.git": true,
        "**/.svn": true,
        "**/.hg": true,
        "**/CVS": true,
        "**/.DS_Store": true,
        "**/.env": true,
        "**/.env.local": true,
        "**/.venv": true,
        "**/node_modules": true,
        "**/__pycache__": true
    }
}